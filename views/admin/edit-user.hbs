
<div class="container-scroller d-flex">
    <!-- partial:./partials/_sidebar.html -->
    {{>admin-header}}
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:./partials/_navbar.html -->
      {{>admin-navbar}}
      <!-- partial -->
      <div class="main-panel">
        <div class="content-wrapper">
    {{!-- all users --}}
<section>
    <div class="container">

      <div class="row mt-4">
         <a href="#" class="btn btn-success ml-auto mb-5">All Users</a>
        </div>
        {{!-- all users --}}
<div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">All User's List</h4>
                  <div class="table-responsive">
                    {{!--  --}}
                    <table class="table mt-5" id="productTable">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile number</th>
      <th scope="col">Option</th>

    </tr>
  </thead>
  <tbody>
      {{#each users}}
    <tr>
      
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.numbers}}</td>
      <td>
       
        {{#if blockStatus}}
        <a href="/admin/unblock-user/{{this._id}}" class="btn btn-success" onclick="return alert('Are you want to Unblock {{this.Name}} ?')">Unblock</a>
        {{else}}
        <a href="/admin/block-user/{{this._id}}" class="btn btn-danger" onclick="return alert('Are you want to Block {{this.Name}} ?')">Block</a>
        {{/if}}
        </td>
    </tr>
    {{/each}}
  </tbody>
</table>
                    {{!--  --}}
                  </div>
                </div>
              </div>
            </div>
          </div>

{{!-- /all users --}}
    </div>
</section>


    {{!-- /all users --}}

          <!-- row end -->
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:./partials/_footer.html -->
       
        <!-- partial -->
      </div>
      <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
